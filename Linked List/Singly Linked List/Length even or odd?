int isLengthEvenOrOdd(struct Node* head)
{
     //Code here
     int count=0;
     Node* current=head;
     while(current!=NULL){
         count++;
         current=current->next;
     }
     if(count%2==0){
         return 0;
     }
     return 1;
}
