#include <iostream>
using namespace std;

int main() {
	//code
	int t;
	cin>>t;
	while(t--){
	    int n,rot;
	    cin>>n>>rot;
	    int arr[n];
	    for(int i=0;i<n;i++){
	        cin>>arr[(i+n-rot)%n];
	    }
	  
	     for( int i=0;i<n;i++){
	        cout<<arr[i]<<" ";
	    }
	    cout<<endl;
	}
	return 0;
}
