void sort012(int arr[], int n)
{
    // coode here 
    int m=0;
    int h=n-1;
    int l=0;
    while(m<=h){
        if(arr[m]==0){
            swap(arr[l],arr[m]);
            l++;
            m++;
        }
        else if(arr[m]==1){
            m++;
        }
        else if(arr[m]==2){
            swap(arr[m],arr[h]);
            h--;
        }
    }

}
