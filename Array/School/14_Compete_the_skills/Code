#include <iostream>
using namespace std;

int main() {
	//code
		int t;
	cin>>t;
	while(t--){
	    int arr1[3];
	    int arr2[3];
	    for(int i=0;i<3;i++){
	        cin>>arr1[i];
	    }
	    for(int j=0;j<3;j++){
	        cin>>arr2[j];
	    }
	    int score1=0;
	    int score2=0;
	    for(int i=0;i<3;i++){
	        if(arr1[i]>arr2[i]){
	            score1++;
	        }
	        else if(arr1[i]==arr2[i]){
	            continue;
	        }
	        else{
	            score2++;
	        }
	    }
	    cout<<score1<<" "<<score2<<endl;
	}
	return 0;
}
