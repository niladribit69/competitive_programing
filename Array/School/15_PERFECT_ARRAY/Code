#include <iostream>
using namespace std;

int main() {
	//code
	int t;
	cin>> t;
	while(t--){
	    int n,flag;
	    cin>>n;
	    int arr[n];
	    for(int i=0;i<n;i++){
	        cin>>arr[i];
	    }
	    for(int i=0;i<n;i++){
	        if(arr[i]==arr[n-1-i]){
	            flag=1;
	        }
	        else{
	            flag=0;
	            break;
	        }
	    }
	    if(flag==1){
	        cout<<"PERFECT"<<endl;
	        }
	   else{
	       cout<<"NOT PERFECT"<<endl;
	   }
	}
	return 0;
}
