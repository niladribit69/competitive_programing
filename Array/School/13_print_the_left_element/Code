#include <iostream>
using namespace std;
#include <bits/stdc++.h>

int main() {
	//code
	int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    int arr[n];
	    for(int i=0;i<n;i++){
	        cin>>arr[i];
	    }
	    sort(arr,arr+n);
	    int mid=n/2;
	    if(n%2!=0){
	        cout<<arr[mid]<<endl;
	    }
	    else{
	        cout<<arr[mid-1]<<endl;
	    }
	}
	return 0;
}
