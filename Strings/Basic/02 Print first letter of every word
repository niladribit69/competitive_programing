class Solution{
public:	
	
	string firstAlphabet(string S)
	{
	    string str="";
	    str+=S[0];
	    for(int i=0;i<S.length();i++){
	        if(S[i-1]==' '){
	            str+=S[i];
	        }
	    }
	    return str;
	}
};
