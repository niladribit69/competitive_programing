#include <iostream>
using namespace std;
#include<set>

int main() {
	//code
	    int T;
    cin >> T;
    
    while(T--)
    {
        string str;
        cin >> str;
        set <char> set_name(begin(str), end(str));
        cout<<str.size()-set_name.size()<<endl;
        
        
    }
	return 0;
}
